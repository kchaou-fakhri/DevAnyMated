{
  "compilerOptions": {
    "strict": true,
    "paths": {
      "@/*": ["./*"],
      "@assets/*": ["./src/assets/*"],
      "@constants/*": ["./src/constants/*"],
      "@contexts/*": ["./src/contexts/*"],
      "@components/*": ["./src/components/*"],
      "@helpers/*": ["./src/helpers/*"],
      "@store/*": ["./src/store/*"],
      "@utils/*": ["./src/utils/*"]
